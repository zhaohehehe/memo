{
  "name": "seed",
  "version": "1.0.0",
  "description": "a seed project for react",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./server/app.js --watch server --watch config --watch client",
    "start-prod": "npm run build && npm run server",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --progress",
    "build-static": "rimraf ./dist && cross-env NODE_ENV=production STATIC=true webpack --progress",
    "server": "node ./dist/server/server.js",
    "lint": "eslint config/**/* server/**/* client/**/* --ext .js,.jsx",
    "lint-fix": "eslint --fix config/**/* server/**/* client/**/* --ext .js,.jsx",
    "pm2": "pm2",
    "pm2-start": "pm2 start process.json",
    "pm2-stop": "pm2 stop process.json",
    "pm2-delete": "pm2 delete process.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
